<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:padding="7dp"
        android:background="@drawable/layout_border_filled"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/dateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:text="Date"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp"/>

    <TextView
        android:id="@+id/dateTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_centerHorizontal="true"
        android:text=":"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/dateTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_alignParentEnd="true"
        android:text=""
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />


    <TextView
        android:id="@+id/clockInTimeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/mulish_medium"
        android:textColor="@color/hrmstext"
        android:layout_marginStart="10dp"
        android:layout_below="@+id/dateTextView"
        android:text="Clock In Time"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/clockInTimeTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_centerHorizontal="true"
        android:text=":"
        android:layout_below="@+id/dateTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />


    <TextView
        android:id="@+id/clockInTimeTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/dateTextView"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/mulish_medium"
        android:textColor="@color/hrmstext"
        android:text=""
        android:textSize="14sp" />

    <TextView
        android:id="@+id/clockOutTimeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_marginTop="5dp"
        android:text="Clock Out Time"
        android:layout_marginStart="10dp"
        android:layout_below="@+id/clockInTimeTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/clockOutTimeTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_centerHorizontal="true"
        android:text=":"
        android:layout_below="@+id/clockInTimeTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/clockOutTimeTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_marginEnd="10dp"
        android:layout_below="@+id/clockInTimeTextView"
        android:layout_marginTop="5dp"
        android:layout_alignParentEnd="true"
        android:text=""
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/totalHoursTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_marginTop="5dp"
        android:layout_marginStart="10dp"
        android:text="Total Hours Worked"
        android:layout_below="@+id/clockOutTimeTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/totalHoursTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_centerHorizontal="true"
        android:text=":"
        android:layout_below="@+id/clockOutTimeTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/totalHoursTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:text=""
        android:layout_below="@+id/clockOutTimeTextView"
        android:layout_alignParentEnd="true"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/lateLoginStatusTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_below="@+id/totalHoursTextView"
        android:layout_marginTop="5dp"
        android:text="Login Status"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/lateLoginStatusTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_centerHorizontal="true"
        android:text=":"
        android:layout_below="@+id/totalHoursTextView"
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />
    <TextView
        android:id="@+id/lateLoginStatusTextView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/hrmstext"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/totalHoursTextView"
        android:text=""
        android:fontFamily="@font/mulish_medium"
        android:textSize="14sp" />

    </RelativeLayout>
</RelativeLayout>